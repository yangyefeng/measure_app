#配置文件等号左右可以有空格也可以没有

[run_mode]
debug=1

#---拌和站配置---
[mix_station_conf]
mix_station_id=1
mix_stationname=路基1
mix_machine_devices=1

#---拌和机配置---
[mix_machine_1]
mix_machine_device_id=1
#mix_machine_type： 1.沥青 2.水泥 3.改性沥青 4.储罐
mix_machine_type=2

#--网络配置--  
[network]
local_ip_add=192.168.1.68
gateway=192.168.1.1
mask=255.255.255.1
dns=8.8.8.8

#--modbus配置--
[modbus_server]
server_ip_add=192.168.1.75
server_ip_port=502


#--料仓、传感器Total配置--

[hopper_sensor]
